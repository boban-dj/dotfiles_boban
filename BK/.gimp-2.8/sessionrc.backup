# GIMP sessionrc
# 
# This file takes session-specific info (that is info, you want to keep
# between two GIMP sessions).  You are not supposed to edit it manually, but
# of course you can do.  The sessionrc will be entirely rewritten every time
# you quit GIMP.  If this file isn't found, defaults are used.

(session-info "toplevel"
    (factory-entry "gimp-empty-image-window")
    (position 410 370)
    (size 620 200))
(session-info "toplevel"
    (factory-entry "gimp-single-image-window")
    (position 0 31)
    (size 1680 964)
    (open-on-exit)
    (aux-info
        (left-docks-width "282")
        (right-docks-width "206")
        (maximized "yes"))
    (gimp-toolbox
        (side left)
        (book
            (current-page 0)
            (dockable "gimp-tool-options"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-dynamics-list"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-palette-list"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-palette-editor"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")
                    (edit-active "true")
                    (current-data "Default")
                    (zoom-factor "2.00")))))
    (gimp-dock
        (side right)
        (book
            (current-page 0)
            (dockable "gimp-layer-list"
                (tab-style automatic)
                (preview-size 32)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-channel-list"
                (tab-style automatic)
                (preview-size 32)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-undo-history"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-dynamics-editor"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "false")
                    (edit-active "true")
                    (current-data "Pressure Opacity")))
            (dockable "gimp-image-list"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true"))))
        (book
            (position 472)
            (current-page 3)
            (dockable "gimp-color-editor"
                (tab-style automatic)
                (aux-info
                    (current-page "ColorselWheel")))
            (dockable "gimp-brush-grid"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-pattern-grid"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-gradient-list"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true"))))))
(session-info "toplevel"
    (factory-entry "gimp-image-new-dialog")
    (position 1883 323))
(session-info "toplevel"
    (factory-entry "gimp-file-export-dialog")
    (position 2138 208)
    (size 672 727))
(session-info "toplevel"
    (factory-entry "gimp-preferences-dialog")
    (position 468 130))
(session-info "toplevel"
    (factory-entry "gimp-input-devices-dialog")
    (position 1921 153))
(session-info "toplevel"
    (factory-entry "gimp-toolbox-color-dialog")
    (position 1680 791))
(session-info "toplevel"
    (factory-entry "gimp-file-save-dialog")
    (position 1680 0)
    (size 672 727))
(session-info "toplevel"
    (factory-entry "gimp-keyboard-shortcuts-dialog")
    (position 586 68)
    (size 786 697))

(hide-docks no)
(single-window-mode yes)
(last-tip-shown 0)

# end of sessionrc
