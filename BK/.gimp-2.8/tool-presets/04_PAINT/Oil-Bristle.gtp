# GIMP tool preset file

(stock-id "gimp-tool-paintbrush")
(name "Oil Bristle")
(tool-options "GimpPaintOptions"
    (tool "gimp-paintbrush-tool")
    (foreground (color-rgb 0.501961 0.149604 0.149604))
    (opacity 0.600000)
    (brush "oil07")
    (dynamics "Paintbrush Basic")
    (gradient "FG to BG (RGB)")
    (palette "Default")
    (brush-size 75.000000)
    (application-mode incremental)
    (jitter-amount 0.350000)
    (dynamics-expanded yes)
    (smoothing-quality 30))
(use-fg-bg no)
(use-brush yes)
(use-dynamics yes)
(use-gradient no)
(use-pattern no)
(use-palette no)
(use-font no)

# end of GIMP tool preset file
